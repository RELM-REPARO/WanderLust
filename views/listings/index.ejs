<% layout('/layouts/boilerplate') %>

    <body>
        <h3>All Listings</h3>
        <ul>
            <form action="/listings/new" method="post">
                <button type="submit">Create new listings</button>
            </form>
            <% for(let listing of allListings){ %>
                <li><a href="/listings/<%= listing._id %>">
                        <%= listing.title%>
                    </a>
                </li>
                <% } %>

        </ul>
    </body>